{
    "name": "group9/secure-ecommerce",
    "description": "Secure E-Commerce Application - Group 9",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "phpmailer/phpmailer": "5.2.23",
        "monolog/monolog": "1.23.0",
        "symfony/http-foundation": "3.4.0",
        "guzzlehttp/guzzle": "6.2.0",
        "doctrine/dbal": "2.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "7.0.0"
    },
    "config": {
        "vendor-dir": "vendor",
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
